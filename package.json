{
  "name": "chrome-extension-block-url",
  "version": "1.0.0",
  "description": "chrome block urls extension",
  "keywords": [
    "block",
    "urls"
  ],
  "license": "ISC",
  "author": "Robin Son",
  "type": "commonjs",
  "main": "webpack.config.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "local": "cross-env NODE_ENV=development webpack --config webpack.config.js",
    "staging": "cross-env NODE_ENV=staging webpack",
    "dev": "mix && npm run local",
    "prod": "mix --production && npm run build",
    "watch": "mix watch && npm run local -- --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^10.0.0",
    "copy-webpack-plugin": "^13.0.0",
    "cross-env": "^7.0.3",
    "laravel-mix": "^6.0.49",
    "postcss": "^8.5.3",
    "sass": "^1.86.3",
    "sass-loader": "^16.0.5",
    "tailwindcss": "^4.1.3",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.3"
  }
}
